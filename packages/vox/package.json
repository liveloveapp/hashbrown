{
  "name": "@hashbrownai/vox",
  "version": "0.0.1",
  "description": "VAD Audio Worklet wrapper with bundled WASM assets",
  "private": true,
  "sideEffects": false,
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "index.*",
    "assets/**/*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.cjs"
    },
    "./loader": {
      "import": "./assets/vad_audio_worklet.single.js",
      "require": "./assets/vad_audio_worklet.single.js",
      "default": "./assets/vad_audio_worklet.single.js"
    },
    "./loader-single": {
      "import": "./assets/vad_audio_worklet.single.js",
      "require": "./assets/vad_audio_worklet.single.js",
      "default": "./assets/vad_audio_worklet.single.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {}
}
