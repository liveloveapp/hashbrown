{
  "name": "anthropic",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/anthropic/src",
  "projectType": "library",
  "release": {
    "version": {
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk",
      "preserveLocalDependencyProtocols": false,
      "manifestRootsToUpdate": ["dist/packages/anthropic", "packages/anthropic"]
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/anthropic",
        "tsConfig": "packages/anthropic/tsconfig.lib.json",
        "packageJson": "packages/anthropic/package.json",
        "main": "packages/anthropic/src/index.ts",
        "assets": [
          "packages/anthropic/*.md",
          {
            "input": ".",
            "glob": "LICENSE",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/anthropic/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/anthropic/jest.e2e.config.ts",
        "passWithNoTests": true
      }
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": ["^nx-release-publish"],
      "options": {
        "packageRoot": "dist/packages/anthropic"
      }
    },
    "build-api-report": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx api-extractor run --local --config packages/anthropic/api-extractor.json",
        "parallel": true
      },
      "dependsOn": [
        {
          "projects": ["anthropic"],
          "target": "build"
        }
      ]
    }
  }
}
