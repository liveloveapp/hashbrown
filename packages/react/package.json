{
  "name": "@hashbrownai/react",
  "version": "0.2.2",
  "description": "React components for Hashbrown AI",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "module": "./index.esm.js",
  "main": "./index.cjs.js",
  "types": "./index.d.ts",
  "exports": {
