    ".": {
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "src/**/*",
    "index.*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "peerDependencies": {
