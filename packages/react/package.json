    "hashbrown",
    "typescript"
  ],
  "homepage": "https://hashbrown.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/liveloveapp/hashbrown.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/liveloveapp/hashbrown/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/liveloveapp"
  }
