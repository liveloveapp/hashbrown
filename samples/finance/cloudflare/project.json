{
  "name": "finance-cloudflare",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "samples/finance/angular/functions",
  "projectType": "application",
  "targets": {
    "generate-data": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx samples/finance/cloudflare/tools/generate-ingredients.ts",
        "parallel": true
      }
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": ["generate-data"],
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "browser",
        "outputPath": "dist/samples/finance/cloudflare",
        "format": ["esm"],
        "bundle": true,
        "thirdParty": true,
        "main": "samples/finance/angular/functions/api/chat.ts",
        "additionalEntryPoints": [
          "samples/finance/angular/functions/api/ingredients.ts"
        ],
        "tsConfig": "samples/finance/cloudflare/tsconfig.app.json",
        "generatePackageJson": false,
        "esbuildOptions": {
          "sourcemap": true,
          "mainFields": ["browser", "module", "main"],
          "conditions": ["browser"],
          "outbase": "samples/finance/angular/functions",
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "finance-cloudflare:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "finance-cloudflare:build:development"
        },
        "production": {
          "buildTarget": "finance-cloudflare:build:production"
        }
      }
    }
  },
  "tags": []
}
