{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.lib.json"
  },
  "messages": {
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "<projectFolder>/tsdoc-metadata.json"
  },
  "apiReport": {
    "enabled": true,
    "reportFolder": "<projectFolder>",
    "reportFileName": "<unscopedPackageName>.api.md"
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": false
  },
  "newlineKind": "lf",
  "bundledPackages": ["@hashbrownai/core"]
}
